{
  "env": {
    "development": {
      "text/typescript": {
        "inlineSourceMap": true,
        "module": "commonjs",
        "target": "es6",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true
      }
    },
    "production": {
      "text/typescript": {
        "inlineSourceMap": false
      }
    }
  }
}