{
    "name": "aurelia-electron-forge",
    "productName": "aurelia-electron-forge",
    "version": "1.0.0",
    "description": "My Electron application description",
    "private": true,
    "main": "index.js",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish"
    },
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "npm"
            },
            "electronWinstallerConfig": {
                "name": "aurelia_electron_forge"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "aureliaelectronforge"
            }
        }
    },
    "dependencies": {
        "aurelia-animator-css": "^1.0.4",
        "aurelia-binding": "^2.1.6",
        "aurelia-bootstrapper": "^2.3.0",
        "aurelia-event-aggregator": "^1.0.1",
        "aurelia-fetch-client": "^1.6.0",
        "aurelia-framework": "^1.3.0",
        "aurelia-history": "^1.1.0",
        "aurelia-history-browser": "^1.2.0",
        "aurelia-loader-nodejs": "^1.0.1",
        "aurelia-logging": "^1.5.0",
        "aurelia-logging-console": "^1.0.0",
        "aurelia-route-recognizer": "^1.3.1",
        "aurelia-router": "^1.6.3",
        "aurelia-templating": "^1.10.1",
        "aurelia-templating-binding": "^1.5.2",
        "aurelia-templating-resources": "^1.7.1",
        "aurelia-templating-router": "^1.3.3",
        "bootstrap": "^3.3.7",
        "electron-compile": "^6.4.3",
        "electron-squirrel-startup": "^1.0.0",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.1"
    },
    "devDependencies": {
        "aurelia-testing": "^1.0.0-beta.3.0.1",
        "babel-preset-env": "^1.6.0",
        "electron-forge": "5.2.3",
        "electron-prebuilt-compile": "3.0.10",
        "typescript": "^3.1.6",
        "@types/node": "^10.12.10",
        "@types/electron": "^1.6.10"
    }
}
